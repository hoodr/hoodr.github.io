{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-typescript-prettier", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "react/require-default-props": "off",
    "react/jsx-props-no-spreading": "off",
    "quotes": ["error", "double"],
    "jsx-quotes": ["error", "prefer-double"]
  }
}
